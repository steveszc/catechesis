setTimeout(async () => {await window.navigator.clipboard.writeText(`answerWithProofs: '${[...[...document.querySelectorAll('b')].find(el => el.textContent === 'A:').nextElementSibling.childNodes].filter(node => node.nodeType === Node.TEXT_NODE || node.nodeName === 'SUP').map(node => node.nodeName === 'SUP' ? `[${node.textContent}] ` : node.textContent.replace(/\n/g, '').trim()).filter(Boolean).join('')}',`);console.log(await window.navigator.clipboard.readText())}, 2000);